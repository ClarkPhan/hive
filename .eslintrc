{
    "extends": [
      "airbnb",
      "airbnb/hooks"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": "warn",
      "jsx-a11y/href-no-hash": ["off"],
      "react/prop-types": ["off"],
      "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
      "max-len": [
        "warn",
        {
          "code": 80,
          "tabWidth": 2,
          "comments": 80,
          "ignoreComments": false,
          "ignoreTrailingComments": true,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true,
          "ignoreRegExpLiterals": true
        }
      ]
    }
  }